   "source": [
    "CREATE OR REPLACE VIEW `breweries-catalog`.gold_layer.breweries_gold AS\n",
    "SELECT\n",
    "    brewery_type,\n",
    "    state,\n",
    "    COUNT(*) AS brewery_count\n",
    "FROM\n",
    "    `breweries-catalog`.silver_layer.breweries_silver\n",
    "WHERE\n",
    "    processing_date = CURRENT_DATE()\n",
    "GROUP BY\n",
    "    brewery_type,\n",
    "    state;"
   ]
  }
